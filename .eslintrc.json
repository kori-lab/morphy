{
  "extends": ["eslint:recommended"],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "requireConfigFile": false
  },
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "no-console": "off",
    "no-empty": "off",
    "no-unsafe-optional-chaining": "off",
    "no-unused-vars": "warn"
  }
}
